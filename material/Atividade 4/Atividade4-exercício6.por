programa
{
	funcao inicio()
	{
		inteiro n1, n2, inter

		escreva("Informe o primeiro número:  \n")
		leia(n1)
		escreva("Informe o segundo número: \n")
		leia(n2)
		escreva("O intervalo entre ", n1, " e ", n2, " é: \n")
		enquanto(n1!=n2){
			n1+=1
			escreva(n1, "\n")
		}
	}
}
