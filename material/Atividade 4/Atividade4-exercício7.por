programa
{
	funcao inicio()
	{
		inteiro n1, n2, soma
		soma=0
		escreva("Informe o primeiro número:  \n")
		leia(n1)
		escreva("Informe o segundo número: \n")
		leia(n2)
		escreva("A soma entre ", n1, " e ", n2, " é: \n")
		enquanto(n1<=n2){
			soma+=n1
			n1+=1
		}
		escreva(soma)
		
	}
}
