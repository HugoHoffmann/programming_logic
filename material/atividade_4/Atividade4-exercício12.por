programa
{
	funcao inicio()
	{
		inteiro n, m, i, j
		escreva("Informe n: \n")
		leia(n)
		escreva("Informe m: \n")
		leia(m)
		i=1
		j=1
		enquanto(i<=n e j<=m){
			escreva(i, "/", j," ")
			i+=1
			j+=2
		}
	}
}
