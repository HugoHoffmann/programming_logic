programa
{
	funcao inicio()
	{
		inteiro n,mul
		escreva("Informe um número: \n")
		leia(n)
		para(inteiro i=1; i<=10; i++){
			mul=n*i
			escreva(n, "x", i, "=", mul, " ")
		}
	}
}
